<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="MainDisplay" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
     <JFXTabPane StackPane.alignment="CENTER_LEFT">
         <tabs>
            <Tab text="Learn" fx:id="learnTab">
               <content>
                   <StackPane fx:id="homepagePlaceholder" />
                  <StackPane prefHeight="150.0" prefWidth="200.0" />
               </content>
            </Tab>
            <Tab text="Test" fx:id="testTab">
               <content>
                   <StackPane fx:id="triviaTestPlaceholder" />
                  <StackPane prefHeight="150.0" prefWidth="200.0" />
               </content>
            </Tab>
            <Tab text="Review" fx:id="reviewTab">
               <content>
                   <StackPane fx:id="reviewPlaceholder" />
                  <StackPane prefHeight="150.0" prefWidth="200.0" />
               </content>
            </Tab>
         </tabs>
      </JFXTabPane>
</StackPane>
