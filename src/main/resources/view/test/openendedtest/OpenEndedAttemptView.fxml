<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" minHeight="105.0" prefHeight="163.0" prefWidth="878.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <HBox prefHeight="61.0" prefWidth="518.0">
      <children>
         <Label text="Question: " />
          <Label fx:id="attemptQuestion" alignment="TOP_LEFT" prefHeight="58.0" prefWidth="335.0" styleClass="cell_big_label" text="\$attemptQuestion" wrapText="true" />
         <Region prefHeight="150.0" prefWidth="25.0" />
         <ImageView fx:id="correctnessIcon" fitHeight="55.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true" />
         <Region prefHeight="150.0" prefWidth="32.0" />
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="426.0">
      <children>
         <Label text="Answer: " />
          <Label fx:id="attemptAnswer" styleClass="cell_big_label" text="\$attemptAnswer" wrapText="true" />
      </children>
   </HBox>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Label text="Your Answer: " />
          <Label fx:id="attemptUserAnswer" styleClass="cell_big_label" text="\$attemptUserAnswer" wrapText="true" />
      </children>
   </HBox>
  </VBox>
